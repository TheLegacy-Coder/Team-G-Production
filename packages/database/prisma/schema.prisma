generator client {
  provider   = "prisma-client-js"
  output     = "../.prisma/client"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Edges {
  edgeID    String @id
  startNode String
  endNode   String
}

model HighScore {
  id    Int      @id @default(autoincrement())
  time  DateTime
  score Int
}

model Nodes {
  nodeID    String @id
  xcoord    Int
  ycoord    Int
  floor     String
  building  String
  nodeType  String
  longName  String
  shortName String
}

model Employee {
  employeeID  String @id
  username    String
  password    String
  job         String
  accessLevel String
}

model ServiceRequest {
  requestID        String @id
  requestType      String
  location         String
  isHandled        String
  assignedEmployee String
}
