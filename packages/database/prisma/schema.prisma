generator client {
  provider   = "prisma-client-js"
  output     = "../.prisma/client"
  engineType = "binary"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Edges {
  edgeID    String @id
  startNode String
  endNode   String
}

model HighScore {
  id    Int      @id @default(autoincrement())
  time  DateTime
  score Int
}

model Nodes {
  nodeID    String @id
  xcoord    Int
  ycoord    Int
  floor     String
  building  String
  nodeType  String
  longName  String
  shortName String
}
